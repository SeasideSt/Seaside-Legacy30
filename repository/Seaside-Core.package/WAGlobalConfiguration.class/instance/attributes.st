attributes
attributes
	^ Array
		with: ((WAListAttribute
			key: #serverProtocol
			group: #server
			comment: 'Overrides the protocol in URLs generated by Seaside to this value.') options: #(http https))
		with: (WAStringAttribute
			key: #serverHostname
			group: #server
			comment: 'Overrides the hostname in URLs generated by Seaside to this value.')
		with: (WANumberAttribute
			key: #serverPort
			group: #server
			comment: 'Overrides the port in URLs generated by Seaside to this value. The default of HTTP is 80, the default of HTTPS is 443.')
		with: (WAStringAttribute
			key: #serverPath
			group: #server
			comment: 'Overrides the path in URLs generated by Seaside to this value.')
		with: (WABooleanAttribute
			key: #deploymentMode
			comment: 'When in deployment mode, the development toolbar is disabled.')